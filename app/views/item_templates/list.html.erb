<h1>Hello, world!</h1>
<div class="main-container">
  <div class="image-row">
    <img src="/profile-pic.jpg" alt="Raghu Betina headshot" class="avatar-image">
  </div>
  
<div class="links-row">
  <% @list_of_items.each do |the_item| %>
    <div class="item">
      <img src="<%= the_item.thumbnail_url %>" class="thumbnail">

      <a class="link-text" target="_blank" href="<%= the_item.link_url %>">
        <%= the_item.link_description %>
      </a>
    </div>
  <% end %>
</div>
